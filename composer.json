{
  "name": "tourze/request-id-bundle",
  "description": "Request ID Generator for Symfony",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^2.17 || ^3.0",
    "symfony/console": "^6.4",
    "symfony/messenger": "^6.4",
    "symfony/uid": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "monolog/monolog": "^3.0",
    "tourze/backtrace-helper": "~0.0.5"
  },
  "autoload": {
    "psr-4": {
      "RequestIdBundle\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}